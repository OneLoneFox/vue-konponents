{
    "name": "konponents",
    "description": "Vue component library",
    "repository": {
        "type": "git",
        "url": "https://github.com/OneLoneFox/vue-konponents",
        "directory": "packages/konponents"
    },
    "version": "1.0.3",
    "main": "dist/konponents.common.js",
    "unpkg": "dist/konponents.umd.min.js",
    "jsdelivr": "dist/konponents.umd.min.js",
    "browser": "dist/konponents.umd.min.js",
    "files": [
        "src/",
        "dist/",
        "vetur/"
    ],
    "scripts": {
        "build": "vue-cli-service build --target lib --name konponents src/main.js",
        "scss:watch": "sass --watch src/styles/main.scss dist/konponents.css",
