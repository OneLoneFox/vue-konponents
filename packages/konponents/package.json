    },
    "dependencies": {},
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "glob": "^7.2.0",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "vue-docgen-api": "^4.41.2",
        "vue-template-compiler": "^2.6.14"
    },
    "peerDependencies": {
        "vue": "^2.6.14"
    },
    "vetur": {
        "tags": "./vetur/tags.json",
        "attributes": "./vetur/attributes.json"
    },
    "license": "MIT",
    "keywords": [
        "konponents",
        "component library",
        "ui library",
        "vue",
        "vuejs",
        "vue2",
